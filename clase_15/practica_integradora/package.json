        "express-handlebars": "^7.1.2",
        "moment": "^2.30.1",
        "mongoose": "^8.4.1",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "eslint": "^8.57.0",
        "eslint-plugin-mongodb": "^1.0.0"
    }
}
