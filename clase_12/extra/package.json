{
    "name": "extra",
    "version": "1.0.0",
    "description": "Servidor de Node JS + Express + Handlebars + Socket.io",
    "main": "src/server.js",
    "type": "module",
    "scripts": {
        "dev": "node --watch --no-warnings ./src/server.js",
        "start": "node ./src/server.js",
        "stop-windows": "taskkill /F /IM node.exe",
        "stop-linux": "killall -9 node",
        "lint": "eslint ./src --fix"
    },
    "engines": {
        "node": ">=16.x"
    },
    "repository": {
        "url": "https://github.com/serinformatico/mi-proyecto-backend.git"
    },
    "keywords": [],
    "author": "Sergio Regalado Alessi",
    "license": "MIT",
    "dependencies": {
        "express": "^4.19.2",
        "express-handlebars": "^7.1.2",
        "socket.io": "^4.7.5"
    },
    "devDependencies": {
        "eslint": "^8.57.0"
    }
}